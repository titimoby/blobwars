function create_blob(x,y,max,r,color)
	baby={}
	baby.x=x
	baby.y=y
	baby.max=max
	baby.r=r
	baby.color=color
	baby.draw=function()
		circ(baby.x,baby.y,baby.max,baby.color)
		circfill(baby.x,baby.y,baby.r,baby.color)
	end
	return baby
end
